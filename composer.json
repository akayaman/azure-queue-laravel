{
  "name": "akayaman/azure-queue-laravel",
  "description": "Laravel 5 Queue Driver for Microsoft Azure Storage Queue",
  "keywords": [
    "laravel",
    "azure",
    "storage",
    "queue",
    "microsoft"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "NAKAYAMA Atsushi",
      "email": "anaka@yo.rim.or.jp"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/queue": "~5.5",
    "microsoft/azure-storage-queue": "~1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0 || ~7.0",
    "orchestra/testbench": "~3.5",
    "mockery/mockery": "~1.0@dev",
    "satooshi/php-coveralls": "~1.0.1"
  },
  "autoload": {
    "psr-4": {
      "Squigg\\AzureQueueLaravel\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Squigg\\AzureQueueLaravel\\AzureQueueServiceProvider"
      ]
    }
  }
}
